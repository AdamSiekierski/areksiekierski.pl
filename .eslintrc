{
  "root": true,
  "env": {
    "node": true
  },
  "extends": [
    "plugin:vue/essential",
    "@vue/airbnb"
  ],
  "rules": {
    "no-console": "off",
    "no-debugger": "off",
    "object-shorthand": 0,
    "quote-props": 0,
    "array-callback-return": "off",
    "max-len": "off",
    "arrow-parens": "off"
  },
  "parserOptions": {
    "parser": "babel-eslint"
  },
}
